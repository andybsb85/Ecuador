<h1>Listing events</h1>

<table>
  <thead>
    <tr>
      <th>String</th>
      <th>City</th>
      <th>String</th>
      <th>Text</th>
      <th>Text</th>
      <th>Date</th>
      <th>Text</th>
      <th>Float</th>
      <th>Float</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.string %></td>
        <td><%= event.city %></td>
        <td><%= event.string %></td>
        <td><%= event.text %></td>
        <td><%= event.text %></td>
        <td><%= event.date %></td>
        <td><%= event.text %></td>
        <td><%= event.float %></td>
        <td><%= event.float %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
